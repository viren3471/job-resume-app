{
  "builds": [
    {
      "src": "ai-resume-analyzer/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "job-scan-main/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/ai-resume-analyzer/main.py"
    },
    {
      "source": "/(.*)",
      "destination": "/job-scan-main/dist/index.html"
    }
  ]
}

